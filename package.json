    "httpervert": "^1.1.5",
    "serve-favicon": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-latest": "^6.24.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "mocha": "^3.2.0",
    "morgan": "^1.8.1",
    "standard": "^9.0.2",
    "webpack": "^2.3.2",
    "winston": "^2.3.1"
